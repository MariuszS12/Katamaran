<!-- Wykres Jonoselektywny -->
<script>
    $(document).ready(function () {

        $.getJSON("/Home/GetData", function (data) {
            //a = data.length - 500;
            //console.log("Długość " + data.length, " -500   " + a);

            var dateTime = []
            var mv0array = []
            var mv1array = []
            var mv2array = []
            var mv3array = []
            var mv4array = []
            var mv5array = []
            var mv6array = []
            for (var i = data.length - 885; i < data.length; i++) {
                dateTime.push(data[i].date);

                mv0array.push(data[i].mv0);
                mv1array.push(data[i].mv1);
                mv2array.push(data[i].mv2);
                mv3array.push(data[i].mv3);
                mv4array.push(data[i].mv4);
                mv5array.push(data[i].mv5);
                mv6array.push(data[i].mv6);




            }




            Highcharts.chart('container', {
                chart: {
                    type: 'line',
                    zoomType: 'x'
                },
                title: {
                    text: 'IonSelectiveMeasurements'
                },

                xAxis: {

                    tickPixelInterval: 400,
                    categories: dateTime
                },



                yAxis: {
                    title: {
                        text: 'Napięcie [mV]'
                    }
                },


                rangeSelector: {
                    floating: true,
                    y: -65,
                    verticalAlign: 'top'
                },





                plotOptions: {
                    line: {
                        dataLabels: {
                            enabled: false
                        },
                        enableMouseTracking: true
                    }
                },
                series: [{
                    name: 'mv0',
                    data: mv0array
                }, {
                    name: 'mv1',
                    data: mv1array
                }, {
                    name: 'mv2',
                    data: mv2array
                }, {
                    name: 'mv3',
                    data: mv3array
                }, {
                    name: 'mv4',
                    data: mv4array
                }, {
                    name: 'mv5',
                    data: mv5array
                }, {
                    name: 'mv6',
                    data: mv6array
                }]
            });
        });
    });


</script>

<!-- Wykres Elmetron -->
<script>
    $(document).ready(function () {

        $.getJSON("/Home/GetDataFromElmetron", function (data) {

            var dateTime = []
            var O2array = []
            var Tarray = []
            var PHarray = []
            var mVarray = []
            var CONarray = []
            //var ID = []
            for (var i = data.length - 918; i < data.length; i++) {
                //ID.push(data[i].ID);
                dateTime.push(data[i].date);
                O2array.push(data[i].O2);
                Tarray.push(data[i].temperature);
                PHarray.push(data[i].PH);
                mVarray.push(data[i].mV);
                CONarray.push(data[i].Conductivity);
                //console.log("DATA = " + i, " oto data : " + data[i].date);
            }
            //console.log("wartość ID = " + ID);



            Highcharts.chart('container2', {
                chart: {
                    type: 'line',
                    zoomType: 'x'
                },
                title: {
                    text: 'ElmetronMeasurement'
                },

                xAxis: {

                    tickPixelInterval: 400,
                    categories: dateTime
                },



                yAxis: {
                    title: {
                        text: 'Temperatura [°C]'
                    }
                },


                rangeSelector: {
                    floating: true,
                    y: -65,
                    verticalAlign: 'top'
                },





                plotOptions: {
                    line: {
                        dataLabels: {
                            enabled: false
                        },
                        enableMouseTracking: true
                    }
                },
                series: [{
                    name: 'O2',
                    data: O2array
                },
                {
                    name: 'PH',
                    data: PHarray
                },
                {
                    name: 'mV',
                    data: mVarray
                },
                {
                    name: 'Conductivity',
                    data: CONarray
                },
                {
                    name: 'T',
                    data: Tarray
                }]
            });
        });
    });


</script>

<!-- Wykres DeepModules -->
<script>
    $(document).ready(function () {

        $.getJSON("/Home/GetDataFromDeepModules", function (data) {

            var dateTime = []
            var TemperatureArray = []

            for (var i = 0; i < data.length; i++) {
                dateTime.push(data[i].date);

                TemperatureArray.push(data[i].Temperature);

            }

            Highcharts.chart('container3', {
                chart: {
                    type: 'line',
                    zoomType: 'x'
                },
                title: {
                    text: 'DeepModules'
                },

                xAxis: {

                    tickPixelInterval: 400,
                    categories: dateTime
                },



                yAxis: {
                    title: {
                        text: 'Temperatura (°C)'
                    }
                },


                rangeSelector: {
                    floating: true,
                    y: -65,
                    verticalAlign: 'top'
                },





                plotOptions: {
                    line: {
                        dataLabels: {
                            enabled: false
                        },
                        enableMouseTracking: true
                    }
                },
                series: [{
                    name: 'Temperature',
                    data: TemperatureArray
                }]
            });
        });
    });


</script>

